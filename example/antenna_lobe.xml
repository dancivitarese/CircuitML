<LpuSpecification xmlns="http://www.neuroml.org/schema/neuroml2"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="cml_v1.xsd" id="Antenna_Lobe">

  <lpuInputIaF id="LpuInputIaF" />
  <lpuOutputIaF id="LpuOutputIaF" />
  <Include href="example/channel.xml" />
  <iafCell id="daniel" reset="-50mV" C="0.03 nF" thresh="-25mV"
		leakConductance="1 uS" leakReversal="-50mV" />

	<network id="antennaLobe">
    <population id="LpuInputIaF" component="LpuInputIaF" size="240" />
    <population id="LpuOutputIaF" component="LpuOutputIaF" size="144" />
    <population id="ch" network="channel" size="48"/>
    <projection id="ch0_ch1">
			<source population="ch[0]/OSN" />
			<target population="ch[1]/GL" />
			<synapticConnection synapse="channel/syn_pn" />
		</projection>
	</network>
</LpuSpecification>
