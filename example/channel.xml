<LpuSpecification xmlns="http://www.neuroml.org/schema/neuroml2"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="cml_v1.xsd" id="channel">
	<expOneSynapse id="syn_osn" erev="20mV" gbase="65nS"
		tauDecay="3ms" />
	<expOneSynapse id="syn_pn" erev="-30mV" gbase="67nS"
		tauDecay="4ms" />
	<iafCell id="osn" reset="-60mV" C="0.03 nF" thresh="-25mV"
		leakConductance="1 uS" leakReversal="-50mV" />
	<iafCell id="pn" reset="-70mV" C="0.03 nF" thresh="-25mV"
		leakConductance="1 uS" leakReversal="-50mV" />
  <network id="channel">
		<population id="GL" component="pn" size="3" />
    <population id="OSN" component="osn" size="5"/>
    <projection id="proj_osn_glomerulus">
      <source population="OSN" />
      <target population="GL" />
      <synapticConnection synapse="syn_osn" />
    </projection>
  </network>
</LpuSpecification>